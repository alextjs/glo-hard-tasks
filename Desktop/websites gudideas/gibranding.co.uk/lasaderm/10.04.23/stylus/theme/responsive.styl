@media only screen and (max-width: 767px)
    /* Layouts */
    #page
        padding-top 80px


    .page-id-17
    .page-id-15
    .page-id-1566
        #page
            padding-top 14px !important
    
    .sec
    #title-bar
        padding-top 40px
        padding-bottom 40px
        
    /* Headings */
    .headline
        font-size 1.8em

    #title-bar .title-bar-heading
    .more-articles .heading
    .testimonial .heading
        font-size 1.6em
    
    .more-articles .heading
