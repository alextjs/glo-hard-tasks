        margin-bottom 25px

    /* UI */
    .action-box
        padding 30px
        
        .heading
            font-size 1.5em
            
        .content
            margin-bottom 15px

        &.action-box-form
            padding 30px

    .btn
    input[type=submit]
    input[type=button] 
    button
        padding 12px 20px
        
    .share-post
        text-align center
        
        .t-a-r
            text-align center
        
        .heading
            margin 0 0 15px
            
        ul
            li
                margin-right 10px
    
    .more-articles .article .article-heading
        font-size 1.1em
        
    .prev-next-post
        text-align center
        
        .t-a-r
            text-align center
        
        .float-right
            float none !important
        
        .gi-col-sm-6
            margin-bottom 30px
            
            &:last-of-type
                margin-bottom 0
    
    .prev-next-post-button .title
        margin-bottom 5px
        
    .testimonial
        .details
            span
                display block
                margin-right 0
                margin-bottom 5px
                float none
                
                &:last-of-type
                    margin-bottom 0

